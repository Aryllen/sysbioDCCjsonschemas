{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"sysbio.metadataTemplates-individual.animal",
    "description": "SysBio individual animal metadata template schema",
    "properties": {
        "individualID": {
            "$ref": "sage.annotations-experimentalData.individualID"
        },
        "individualIdSource": {
            "$ref": "sage.annotations-experimentalData.individualIdSource"
        },
        "species": {
            "$ref": "sage.annotations-experimentalData.species"
        },
        "sexChromosome": {
            "$ref": "sage.annotations-clinical.sexChromosome"
        },
        "dateOfBirth": {
            "$ref": "sage.annotations-demographics.dateOfBirth"
        },
        "dateOfDeath": {
            "$ref": "sage.annotations-demographics.dateOfDeath"
        },
        "treatmentType": {
            "$ref": "sage.annotations-neuro.treatmentType"
        }
    }
}
